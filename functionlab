function fizzBuzz(x){
  if (x == 3){
    return 'Fizz';
  }
  else if (x == 4){
    return 4;
  }
  else if(x == 5){
    return "Buzz"
  }
  else if (x == 11){
    return 11;
  }
  else if (x == 15){
    return "FizzBuzz"
  }
}

console.log (fizzBuzz(3)); // "Fizz"
console.log (fizzBuzz(4));// 4
console.log (fizzBuzz(5));// "Buzz"
console.log (fizzBuzz(11));// 11
console.log (fizzBuzz(15));// "FizzBuzz"
